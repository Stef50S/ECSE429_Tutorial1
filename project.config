[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Stefano Commodari (Stef50S)
[access "refs/for/refs/*"]
	read = group user/Stefano Commodari (Stef50S)
	push = group user/Stefano Commodari (Stef50S)
	label-Code-Review = -2..+2 group user/Stefano Commodari (Stef50S)
	label-Verified = -1..+1 group user/Stefano Commodari (Stef50S)
	rebase = group user/Stefano Commodari (Stef50S)
	removeReviewer = group user/Stefano Commodari (Stef50S)
	submit = group user/Stefano Commodari (Stef50S)
[access "refs/heads/*"]
	read = group user/Stefano Commodari (Stef50S)
	create = group user/Stefano Commodari (Stef50S)
	push = +force group user/Stefano Commodari (Stef50S)
	pushMerge = group user/Stefano Commodari (Stef50S)
[access "refs/tags/*"]
	push = group user/Stefano Commodari (Stef50S)
	push = +force group user/Stefano Commodari (Stef50S)
